						//提交并验证表单
						$('.submitBtn').click(function() {
							var EmailReg = /^[0-9a-zA-Z]+(?:[\.\!\#\$\%\^\&\*\'\+\-\/\`\_\{\|\}\~]{0,1}[a-zA-Z0-9]+)*@[a-zA-Z0-9\-]+\.[0-9a-zA-Z\-]+$/; //邮件正则
							var PhoneReg = /^0{0,1}(13[0-9]|15[0-9]|153|156|18[7-9])[0-9]{8}$/ ; //手机正则
							var $title = $('.title');
							var $email = $('.email');
							var $phone = $('.phone');
							if($title.val() == ''){
								$.tooltip('昵称还没填呢...'); $title.focus();
							}else if($phone.val() == ''){
								$.tooltip('手机还没填呢...'); $phone.focus();
							}else if(!PhoneReg.test($phone.val())){
								$.tooltip('手机格式错咯...'); $phone.focus();
							}else if($email.val() == ''){
								$.tooltip('邮箱还没填呢...'); $email.focus();
							}else if(!EmailReg.test($email.val())){
								$.tooltip('邮箱格式错咯...'); $email.focus();
							}else{
								$.tooltip('提交成功，您的加盟信息正在提交中...',5000,true);
								setTimeout(function(){
									//$el.hDialog('close',{box:'#HBox'},'http://smwell.sinaapp.com/');		//也可以加跳转链接哦~
									$el.hDialog('close',{box:'#HBox'});
								},2000);
							}
						});
