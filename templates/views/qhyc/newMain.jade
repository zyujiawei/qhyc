							script(type='text/javascript').
								$(function(){
									$('#msg').submit(function(){
										$this=$(this),
										$title=$('#title'),
										$email=$('#email'),
										$content=$('#content'),
										$tip=$('#error'),
										$btn=$('#submit'),
										url=$this.attr('action'),
										title=$title.val(),
										email=$email.val(),
										msgContent=$content.val();
										if($btn.attr('data_submited')==1){
											return false;
										}
										if(title==''){
											$tip.html('姓名不能为空').show();
											$title.focus();
											return false;
										}else if(email==''){
											$tip.html('请填写邮箱，以便我们及时联系你').show();
											$email.focus();
											return false;
										}else{
											var regx=new RegExp("^\\\w+((-\\w+)|(\\.\\w+))*\\@[A-Za-z0-9]+((\\.|-)[A-Za-z0-9]+)*\\.[A-Za-z0-9]+$");
											if(regx.test(email)){
												$tip.html('');
											}else{
												$tip.html('邮箱格式不正确').show();
												return false;
											}
										}
										if(msgContent==''){
											$tip.html('把你的需求大概告诉我们').show();
											$content.focus();
											return false;
										}
										$btn.attr('data_submited',1).val('正在提交');
										$.post(url,$this.serialize(),function(data) {
											if(data.status!=1){
												$tip.html(data.info).show();
											}
											$title.val('');
											$email.val('');
											$content.val('');
											$btn.attr('data_submited',0).val('提交');
										});
										return false;
									});
									$(window).scroll(function(){
										var scrollTopPx=$(window).scrollTop(),
										bodyHeight=$('body').height();
										if(scrollTopPx>bodyHeight/2){
											$('#goTopBtn').show();
										}else{
											$('#goTopBtn').hide();
										}
									});
									$('body').delegate('#goTopBtn','click',function(){
										$('html,body').animate({scrollTop:0},800,function(){
											$('#goTopBtn').hide();
										});
									});
